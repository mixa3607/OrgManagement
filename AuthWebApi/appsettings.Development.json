{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AuthOptions": {
    "JwtIssuer": "AuthDevServer",
    "JwtAudience": "AuthDevClient",
    "JwtLifetimeMins": 5,
    "RefreshTokenLifetimeDays": 50,
    "PublicCrtPath": "./jwt.crt",
    "PrivatePfxPath": "./jwt.pfx"
  }
}
